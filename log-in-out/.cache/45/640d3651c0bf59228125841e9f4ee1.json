nts.googleapis.com/css2?family=Maven+Pro:wght@400;500&display=swap\");\n\nbody {\n  box-sizing: border-box;\n  height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-family: \"Maven Pro\", sans-serif;\n}\n\n.wrapper {\n  border-radius: 10px;\n  background: linear-gradient(#2a5967, #4e7f5f);\n}\n\n.design {\n  margin: 30px;\n  position: relative;\n  box-shadow: 15px 12px 13px 0px #213b42b8;\n  border-top: 2px solid #79969a;\n  border-radius: 4px;\n}\n\n.log-in {\n  position: absolute;\n  // top: 1px;\n  z-index: 2;\n  width: 100%;\n  height: 101%;\n  clip-path: polygon(0% 0%, 0% 100%, 100% 100%, 100% 90%, 15% 0%);\n  border-radius: 4px;\n  background: linear-gradient(#9fb1b6, #084555);\n}\n\nh1 {\n  font-size: 40px;\n  margin: 2.5em 1.3em 1.5em 1.3em;\n  color: #ffffff;\n}\n\n.sign-up-form {\n  margin: 3.5em 0 0 52px;\n  display: flex;\n  flex-flow: column nowrap;\n  width: 45%;\n\n  input {\n    color: #ffffff;\n    outline: none;\n    border: none;\n    border-bottom: 1px solid;\n    width: 60%;\n    background: none;\n    font-size: 17px;\n    padding: 3px 0;\n\n    &::placeholder {\n      color: #335a69;\n      font-size: 15px;\n    }\n  }\n\n  label {\n    margin-top: 3px;\n    margin-bottom: 20px;\n    font-size: 15px;\n    color: #99d1da;\n  }\n}\n\n.ckbox {\n  display: flex;\n  flex-direction: row;\n\n  p {\n    color: #ffffff;\n  }\n\n  input {\n    width: fit-content;\n    margin: 3px 10px 30px 0;\n    -webkit-appearance: none;\n    -moz-appearance: none;\n    background-color: #07ee2d;\n    border: 1px solid #cacece;\n    padding: 6px;\n    border-radius: 2px;\n    position: relative;\n\n    &:checked {\n      background-color: #07ee2d;\n      border: 1px solid #adb8c0;\n    }\n\n    &:checked::after {\n      content: \"\\2714\";\n      font-size: 16px;\n      position: absolute;\n      top: -3px;\n      left: 0px;\n      color: black;\n    }\n\n    &:hover {\n      cursor: pointer;\n    }\n  }\n}\n\n#submit {\n  width: 60%;\n  padding: 8px;\n  border: none;\n  background: #4af74a;\n  border-radius: 3px;\n  outline: none;\n  box-shadow: 16px 11px 4px 2px #022f3b;\n  transition: box-shadow 0.3s ease-in;\n  font-size: 18px;\n  color: #044e5e;\n\n  &:hover {\n    cursor: pointer;\n    box-shadow: 4px 6px 4px 2px #022f3b;\n  }\n}\n\n.wallwaper {\n  width: 700px;\n  height: 500px;\n  background-image: url(\"./img/backgroud-img4.jpg\");\n  background-position: bottom;\n  background-size: cover;\n  border-radius: 4px;\n}\n"},"lineCount":null}},"error":null,"hash":"9ced160b0a6668b7d35423b8220252a5","cacheData":{"env":{}}}